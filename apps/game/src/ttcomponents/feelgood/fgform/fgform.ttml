<view class="fg-form {{ themeClass }}">   <scroll-view     id="questionList"     class="scroll-view"     scroll-y="true"     style="max-height: {{ scrollViewMaxHeight }}px;"     scroll-into-view="{{ focusedQuestionId }}"   >     <view class="fg-question-list">       <block tt:for="{{ visibleQuestions }}">         <nps           tt:if="{{ item.type === 5 }}"           class="question question-nps {{ themeClass }}"           id="q{{ item.id }}"           sequence="{{ index + 1 }}"           config="{{ item }}"           bindValueChange="onFormValueChange"         ></nps>         <rating           tt:elif="{{ item.type === 4 }}"           class="question quesiton-rating"           id="q{{ item.id }}"           sequence="{{ index + 1 }}"           config="{{ item }}"           bindValueChange="onFormValueChange"         ></rating>         <true-or-false           tt:elif="{{ item.type === 6 }}"           class="question question-boolean"           id="q{{ item.id }}"           sequence="{{ index + 1 }}"           config="{{ item }}"           bindValueChange="onFormValueChange"         ></true-or-false>         <multi-select           tt:elif="{{ item.type === 3 }}"           class="question question-multi-select"           id="q{{ item.id }}"           sequence="{{ index + 1 }}"           config="{{ item }}"           bindValueChange="onFormValueChange"           bindScrollToQuestion="onScrollIntoView"         ></multi-select>         <single-select           tt:elif="{{ item.type === 2 }}"           class="question question-single-select"           id="q{{ item.id }}"           sequence="{{ index + 1 }}"           config="{{ item }}"           bindValueChange="onFormValueChange"           bindScrollToQuestion="onScrollIntoView"         ></single-select>         <short-answer           tt:elif="{{ item.type === 1 }}"           class="question question-short-answer"           id="q{{ item.id }}"           sequence="{{ index + 1 }}"           config="{{ item }}"           bindValueChange="onFormValueChange"           bindScrollToQuestion="onScrollIntoView"         ></short-answer>         <plain-text           tt:else           class="question question-plain-text {{ themeClass }}"           id="q{{ item.id }}"           sequence="{{ index + 1 }}"           config="{{ item }}"         ></plain-text>       </block>     </view>   </scroll-view>   <view     class="fg-form__footer"     catchtouchmove="doNothing"   >     <view       tt:if="{{ !onlyOneRadioQuestion }}"       class="fg-form__footer-submit-btn {{ isAllRequiredFilled? 'normal-btn' : 'disabled-btn' }}"       bindtap="onClickSubmit"     >       <text class="submit-text">         {{ labels.submit_btn }}       </text>     </view>      <view       tt:if="{{ taskConfig.common_config && taskConfig.common_config.show_block_btn }}"       bindtap="onTapDoNotShowAgain"       class="do_not_show_again"     >       <text class="do_not_show_again__text">         {{ blockBtnLabel }}       </text>     </view>   </view> </view>
