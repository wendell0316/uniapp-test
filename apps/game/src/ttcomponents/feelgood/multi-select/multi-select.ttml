<question-template   class="fg-multi-select"   config="{{ config }}"   sequence="{{ sequence }}" >   <view     tt:if="{{ config.display_style === 'tag' }}"     class="fg-multi-select__body"   >     <tag-option       tt:for="{{ options }}"       tt:for-item="option"       class="tag-option"       style="min-width: 48%;"       config="{{ option.optionConfig }}"       selected="{{ option.selected }}"       disabled="{{ option.disabled }}"       bindTapOption="onToggleOption"     ></tag-option>   </view>   <view tt:else class="fg-multi-select__body">     <line-option       tt:for="{{ options }}"       tt:for-item="option"       style="width: 100%;"       boxStyle="{{ boxStyle }}"       questionId="{{ config.id }}"       config="{{ option.optionConfig }}"       selected="{{ option.selected }}"       disabled="{{ option.disabled }}"       bindTapOption="onToggleOption"       bindExtraChange="onExtraChange"       bindScrollToQuestion="onScrollToQuestion"     ></line-option>   </view> </question-template>
