@font-face {
  font-family: 'DIN Alternate Bold';
  src: url(data:application/x-font-ttf;charset=utf-8;base64,) format('truetype');
}
.fg-number-option {
  display: flex;
  box-sizing: border-box;
  justify-content: center;
  align-items: center;
  width: 100%;
  height: 30px;
  border-radius: 4px;
  border: 1px solid;
}
.normal-option {
  border-color: rgba(22, 24, 35, 0.12);
}
.selected-option {
  border-color: var(--flgd-theme-primary, #fe2c55);
}
.text {
  color: rgba(22, 24, 35, 0.75);
  font-size: 13px;
  line-height: 1;
}
.selected-text {
  color: var(--flgd-theme-primary, #fe2c55);
}
.theme-people .fg-number-option {
  height: 36px;
}
.theme-people .normal-option {
  border-color: #dee0e3;
}
.theme-people .selected-option {
  border-color: var(--flgd-theme-primary, #3370ff);
  background-color: var(--flgd-theme-primary, #3370ff);
}
.theme-people .text {
  font-family: DIN Alternate Bold;
  font-size: 16px;
  color: #1f2329;
}
.theme-people .selected-text {
  color: white;
}
