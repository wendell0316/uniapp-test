<question-template   class="fg-rating {{ themeClass }}"   config="{{ config }}"   sequence="{{ sequence }}" >   <view class="fg-rating__body">     <view       tt:if="{{ config.display_style === 'number' }}"       class="fg-rating__options-number"     >       <number-option         tt:for="{{ options }}"         tt:for-item="option"         class="number-option"         config="{{ option.optionConfig }}"         selected="{{ option.selected }}"         bindTapOption="onSelectOption"       ></number-option>     </view>     <view tt:else class="fg-rating__options-star">       <star-option         tt:for="{{ options }}"         tt:for-item="option"         config="{{ option.optionConfig }}"         selected="{{ option.selected }}"         highlight="{{ !!selectedOption ? option.optionConfig.rate <= selectedOption.rate : false }}"         bindTapOption="onSelectOption"       ></star-option>     </view>     <view       tt:if="{{ selectedOption }}"       class="center-label-container"     >       <text class="center-label-text">         {{ selectedOption.content }}       </text>     </view>   </view> </question-template>
