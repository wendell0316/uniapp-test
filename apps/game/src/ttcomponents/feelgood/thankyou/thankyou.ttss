.thankyou-panel {
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 100%;
  height: 100%;
}
.thankyou-panel__logo {
  margin-bottom: 11px;
}
.thankyou-panel__text {
  color: #333;
  font-weight: 600;
  font-size: 14px;
  line-height: 32px;
}
.theme-people .thankyou-panel__logo {
  margin-bottom: 16px;
}
.theme-people .thankyou-panel__text {
  color: #646a73;
  font-size: 14px;
  line-height: 22px;
  font-weight: normal;
  margin-bottom: 28px;
}
