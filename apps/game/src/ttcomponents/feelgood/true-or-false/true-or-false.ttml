<question-template   class="fg-true-or-false"   config="{{ config }}"   sequence="{{ sequence }}" >   <view class="fg-true-or-false__body">     <view class="fg-thumb" bindtap="onSelectTrue">       <fgicon         content="{{ 'thumb-aweme' }}"         class="fg-thumb-icon up"         color="{{ hasTrueSelected? isLynxRuntime? '#fe2c55' :'var(--flgd-theme-primary, #fe2c55)' : 'rgba(22, 24, 35, 0.75)' }}"       ></fgicon>       <text class="fg-thumb-label {{ hasTrueSelected? 'selected' : 'normal' }}">         {{ config.options[0].content }}       </text>     </view>     <view class="spacer"></view>     <view class="fg-thumb" bindtap="onSelectFalse">       <fgicon         content="{{ 'thumb-aweme' }}"         class="fg-thumb-icon down"         color="{{ hasFalseSelected? isLynxRuntime? '#fe2c55' :'var(--flgd-theme-primary, #fe2c55)' : 'rgba(22, 24, 35, 0.75)' }}"       ></fgicon>       <text class="fg-thumb-label {{ hasFalseSelected? 'selected' : 'normal' }}">         {{ config.options[1].content }}       </text>     </view>   </view> </question-template>
